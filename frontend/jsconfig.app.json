{
    "compilerOptions": {
        "target": "es6",
        "module": "esnext",
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"]
        },
        "jsx": "react-jsx",
        "allowJs": true,
        "checkJs": false,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true
    },
    "include": ["src"]
}